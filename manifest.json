{
    "name": "Request forwarder",
    "short_name": "r-fowarder",
    "description": "转发 http 请求",
    "version": "0.0.1",
    "icons": {
        "16": "icons/request-16.png",
        "48": "icons/request-48.png",
        "64": "icons/request-64.png",
        "128": "icons/request.png"
    },
    "permissions": [
        "webRequest",
        "storage",
        "webRequestBlocking",
        "browsingData",
        "<all_urls>"
    ],
    "browser_action": {
        "default_title": "r-forwarder",
        "default_icon": "icons/request.png",
        "default_popup": "./pages/popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "manifest_version": 2,
    "homepage_url": "https://github.com/neal1991/image-host",
    "options_page": "./pages/options.html",
    "content_security_policy": "script-src 'self' 'sha256-dqxE6cbFBlR62LlsAs4nkntJmzQVdovSF+cSwQVawt4='"
}